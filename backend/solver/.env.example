# SWE-agent Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_API_BASE=https://openrouter.ai/api/v1

# SWE-agent Configuration Paths
SWEAGENT_CONFIG_PATH=/app/solver/config.yaml
SWEAGENT_DATA_PATH=/data/swe_runs
SWEAGENT_LOG_LEVEL=INFO

# Solver Resource Limits
MAX_SOLVE_TIME_MINUTES=30
MAX_CONCURRENT_SOLVES=3
SOLVER_TIMEOUT_SECONDS=1800
MAX_SOLVER_COST_USD=10.0

# Docker Configuration
DOCKER_IMAGE=sweagent/swe-agent:latest
DOCKER_MEMORY_LIMIT=2g
DOCKER_CPU_LIMIT=2

# Repository Cloning
GIT_CLONE_TIMEOUT=300
GIT_CLONE_DEPTH=1
DEFAULT_BRANCH=main

# Safety and Security
ENABLE_NETWORK_ACCESS=false
ALLOWED_FILE_EXTENSIONS=.py,.js,.ts,.jsx,.tsx,.java,.cpp,.c,.h,.hpp,.cs,.go,.rs,.rb,.php,.swift,.kt,.scala,.clj,.hs,.ml,.fs,.vb,.sql,.html,.css,.scss,.less,.json,.yaml,.yml,.xml,.md,.txt,.sh,.bat,.ps1,.dockerfile,.makefile,.cmake,.gradle,.maven,.sbt,.cargo,.gemfile,.requirements,.package,.lock

# Logging Configuration
LOG_TRAJECTORY=true
LOG_COMMANDS=true
LOG_OUTPUTS=true
LOG_ERRORS=true
LOG_FILE_PATH=/data/swe_runs/solver.log
LOG_ROTATION_SIZE=10MB
LOG_RETENTION_DAYS=7

# Performance Tuning
CACHE_ENABLED=true
CACHE_SIZE_MB=256
PARALLEL_PROCESSING=false
MAX_PARALLEL_TASKS=1

# Development Settings (set to false in production)
DEBUG_MODE=false
VERBOSE_LOGGING=false
SAVE_INTERMEDIATE_RESULTS=true
ENABLE_PROFILING=false

# Database Connection (if needed for direct DB access)
DATABASE_URL=postgresql://yudai_user:yudai_password@db:5432/yudai_db

# Monitoring and Metrics
ENABLE_METRICS=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30

# Backup and Recovery
BACKUP_TRAJECTORIES=true
BACKUP_RETENTION_DAYS=30
AUTO_CLEANUP_TEMP_FILES=true

# Integration Settings
WEBHOOK_URL=
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=

# Feature Flags
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_ADVANCED_ANALYSIS=true
ENABLE_CODE_SUGGESTIONS=true
ENABLE_AUTOMATIC_TESTING=false
ENABLE_DOCUMENTATION_GENERATION=true

# Rate Limiting
REQUESTS_PER_MINUTE=60
BURST_LIMIT=10
COOLDOWN_PERIOD_SECONDS=5

# Error Handling
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY_SECONDS=5
FAIL_FAST_ON_CRITICAL_ERRORS=true

# Notification Settings
NOTIFY_ON_COMPLETION=true
NOTIFY_ON_FAILURE=true
NOTIFY_ON_TIMEOUT=true
EMAIL_NOTIFICATIONS=false
SMTP_SERVER=
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=